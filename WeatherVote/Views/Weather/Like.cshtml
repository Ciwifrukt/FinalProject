@model WeatherVM


@foreach (var w in Model.Weathers)
{
    <div class="alert" style="background-color:aqua">
        <h1>@w.Supplier.Name</h1>
        <p>@w.Temperatur</p>
        <p> @w.Description </p>
        <p>Wind: @w.Wind </p>
        <p>Humidity: @w.Humidity </p>
        <form method="post">
            <button type="submit" asp-action="AddLikes" asp-route-wname="@w.Supplier.Name" asp-route-loc="@w.Loc.CityName">Gilla</button>
        </form>
    </div>
}